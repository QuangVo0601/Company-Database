   <children>
      <Button fx:id="loginButton" layoutX="120.0" layoutY="111.0" mnemonicParsing="false" onAction="#checkValidSsn" text="LOGIN" />
      <TextField fx:id="managerSSN" layoutX="120.0" layoutY="65.0" promptText="Enter your SSN" />
      <Button fx:id="resetButton" layoutX="228.0" layoutY="111.0" mnemonicParsing="false" onAction="#reset" text="RESET" />
   </children>
</AnchorPane>
